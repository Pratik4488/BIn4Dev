<div class="buttonsContainer">
    <a href="/" class="logo">CodeBin</a>
    <div class="buttons">
        <a href="/new" class="btn">New</a>
        <% if(locals.canSave) {%>
            <button type="submit" class="btn">Save</button>
         <% } %>
        <% if(locals.id!= null) {%>
            <a href="/<%= locals.id %>/duplicate " class="btn">Duplicate</a>
            <% } %> 
            <% if(locals.dashboard) {%>
                <a href="/users/dashboard/" class="btn">Dashboard</a>
             <% } %>
            <% if(locals.encrypt) {%>
                <a href="/users/dashboard/" class="btn">Encrypt</a>
             <% } %>
        <a href="/users/logout" class="btn">LogOut</a>
    </div>
</div>